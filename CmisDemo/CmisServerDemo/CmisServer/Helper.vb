Public Module Helper

   Public Function FindXmlPath(filename As String) As String
      Dim path As String = filename
      If Not IO.File.Exists(path) Then
         path = "Xml\" & path
         While Not IO.File.Exists(path)
            path = "..\" & path
            If path.Length > 400 Then Throw New Exception("Xml-Vorlage '" & filename & "' nicht gefunden!")
         End While
      End If

      Return path
   End Function

